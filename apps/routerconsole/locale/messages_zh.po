msgstr "指定主机名或IP"

#: ../jsp/WEB-INF/classes/net/i2p/router/web/jsp/config_jsp.java:419
msgid "Select Interface"
msgstr "选择网络接口"

#: ../jsp/WEB-INF/classes/net/i2p/router/web/jsp/config_jsp.java:419
#: ../jsp/WEB-INF/classes/net/i2p/router/web/jsp/configservice_jsp.java:370
